{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@core/*": [
        "src/app/core/*"
      ],
      "@core-service/*": [
        "src/app/core/services/*"
      ],
      "@home/*": [
        "src/app/feature/home/*"
      ],
      "@cita/*": [
        "src/app/feature/cita/*"
      ],
      "@eps/*": [
        "src/app/feature/eps/*"
      ],
      "@medico/*": [
        "src/app/feature/medico/*"
      ],
      "@paciente/*": [
        "src/app/feature/paciente/*"
      ],
      "@administracion/*": [
        "src/app/feature/administracion/*"
      ],
      "@login/*": [
        "src/app/feature/login/*"
      ],
      "@shared/*": [
        "src/app/shared/*"
      ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
